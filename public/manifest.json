{
  "manifest_version": 3,
  "name": "MRU Tabs",
  "author": "jharcombe@atlassian.com",
  "version": "0.1.0",
  "description": "A MRU Tab switcher",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "prevTab": {
      "suggested_key": "Ctrl+Shift+Q",
      "description": "Select previous tab in queue"
    },
    "nextTab": {
      "suggested_key": "Ctrl+L",
      "description": "Select next tab in queue"
    }
  },
  "permissions": ["tabs"]
}
